@model IEnumerable<CarrosModel>

<h1 class="display-4">Informações sobre os Carros</h1>

<table class="table table-hover table-dark">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">Marca</th>
            <th scope="col">Modelo</th>
            <th scope="col">Cor</th>
            <th scope="col">Ano</th>
            <th scope="col">Data de atualização</th>
        </tr>
    </thead>
    <tbody>

        @if (Model != null)
        {
            @foreach (var registo in Model)
            {

                <tr>
                    <th scope="row">@registo.Id</th>
                    <td>@registo.Marca</td>
                    <td>@registo.Modelo</td>
                    <td>@registo.Cor</td>
                    <td>@registo.Ano</td>
                    <td>@registo.DataUltimaAtualizacao</td>
                </tr>
            }
        }
    </tbody>







</table>